//Создайте программу, которая с использованием циклов выводит последовательность: 5, 12, 19, 26, 33, 40, 47, 54, 61, 68, 75, 82, 89, 96, 103. Объясните, почему выбрали именно этот цикл. 
//Используйте переменные для обозначения чисел в условии цикла. Подсчитывать количество итераций не нужно. Даже если максимальное число изменится на 789, в коде нужно будет изменить только это значение.

using System;

namespace Sequence
{
    class Program
    {
        static void Main(string[] args)
        {
           
            int lowerBound = 5;
            int upperBound = 103;
            int cycleStep = 7;

            for (int i = lowerBound; i < upperBound; i += cycleStep)
            {
                Console.Write(i + " ");
            }
        }
    }
}
